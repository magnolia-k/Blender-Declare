#!/usr/bin/perl

use 5.012;
use warnings;

use Test::More;
use Test::Blender::Definition;

SKIP: {
          skip "Skip build Rakudo Star test because none of test env.",
               3 unless ( $ENV{PERL_BLENDER_TEST_DEFINITION} );
 
          build_ok( 'rakudostar', undef, undef, 'first test' );
      };

done_testing();
